h2 {
	font-family: Georgia, 'Times New Roman', Times, serif;
	color: #522B29;
	text-shadow: 5px 5px 6px gray;
}
body {
	background-color: #FDC197;
}
	
div {
	border: 1px solid #000;
	margin: 110px;
	padding: 110px;
	background-color: white;
}
img.pic {
	float: left;
	width: 150px;
	background-color: #F9F9F9;
}	
ol {
	list-style-type: disc;
	background-color: #F9F9F9;
}
ul {
	list-style-type: disc;
	background-color: #F9F9F9;
}
p.head {
	background-color: #F9F9F9;
}
p.title {
	font-weight: strong;
	background-color: #8D4202;
	color: white;
}
p.personalinfo {
	background-color: #F9F9F9;
}